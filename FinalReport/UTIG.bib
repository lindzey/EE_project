@article{Fairfield2007c,
abstract = {We describe a Simultaneous Localization and Mapping (SLAM) method$\backslash$nfor a hovering underwater vehicle that will explore underwater caves$\backslash$nand tunnels, a true three dimensional (3D) environment. Our method$\backslash$nconsists of a Rao-Blackwellized particle filter with a 3D evidence$\backslash$ngrid map representation. We describe a procedure for dynamically$\backslash$nadjusting the number of particles to provide real-time performance.$\backslash$nWe also describe how we adjust the particle filter prediction step$\backslash$nto accommodate sensor degradation or failure. We present an efficient$\backslash$noctree data structure which makes it feasible to maintain the hundreds$\backslash$nof maps needed by the particle filter to accurately model large environments.$\backslash$nThis octree structure can exploit spatial locality and temporal shared$\backslash$nancestry between particles to reduce the processing and storage requirements.$\backslash$nTo test our SLAM method, we utilize data collected with manually-deployed$\backslash$nsonar mapping vehicles in the Wakulla Springs cave system in Florida$\backslash$nand the Sistema Zacat n in Mexico, as well as data collected by the$\backslash$nDEPTHX vehicle in the test tank at the Austin Applied Research Laboratory.$\backslash$nWe demonstrate our mapping and localization approach with these real-world$\backslash$ndatasets.},
author = {Fairfield, Nathaniel and Kantor, George and Wettergreen, David},
doi = {10.1002/rob.20165},
file = {:Users/lindzey/Documents/UTIG/Writing/References/UTIG/Fairfield2007a.pdf:pdf},
isbn = {1556-4959},
issn = {15564959},
journal = {Journal of Field Robotics},
number = {1-2},
pages = {3--21},
title = {{Real-time SLAM with octree evidence grids for exploration in underwater tunnels}},
volume = {24},
year = {2007}
}
