@article{Herzfeld2002a,
abstract = {Vario functions are lag-dependent experimental functions introduced to characterize spatial properties of surfaces and profiles in a general algebraic sense and their derivatives. Vario functions of higher order are defined recursively. Under certain mathematical conditions, first-order vario functions coincide with experimental variograms. Vario functions of higher order provide a useful extension of the geostatistical classification method, in particular in the cases of poor signal-to-resolution ratio and in the cases of complex morphogenesis. Several theoretical questions concerning vario functions are investigated: feature detection and quantitative analysis in a situation of poor signal-to-resolution ratio, the divergence criterion for determination of the usable distance range of the second-order vario function, characterization of superimposed features (of the geo-variable) by secondary minima and convexity or concavity of the (second-order) vario function, effects of increasing resolution in vario-function calculation for the study of small-scale features, and the question of homogeneity of a data set. Higher-order vario functions prove essential in the characterization of snow surface roughness, demonstrated for microtopographic data from Niwot Ridge, Colorado Front Range. Application of the geostatistical classification approach to vario functions of higher order leads to conclusions on snow surface morphogenesis and its dependence on environmental and geographical conditions that are not obtainable otherwise. ?? 2002 Published by Elsevier Science Ltd.},
author = {Herzfeld, Ute Christina},
doi = {10.1016/S0098-3004(01)00096-6},
file = {:Users/lindzey/Documents/UTIG/Writing/References/UTIG/Herzfeld2002.pdf:pdf},
issn = {00983004},
journal = {Computers and Geosciences},
keywords = {Geostatistical classification,Higher-order vario function,Microtopography,Res function,Resvar function,Snow hydrology,Variogram},
number = {5},
pages = {641--660},
title = {{Vario functions of higher order - Definition and application to characterization of snow surface roughness}},
volume = {28},
year = {2002}
}
