abstract = {During the last interglacial period, 127–116 kyr ago, global mean sea level reached a peak of 5–9 mabove present-day sea level. However, the exact timing and magnitude of ice sheet collapse that contributed to the sea-level highstand is unclear. Here we explore this timing using stratigraphic and geomorphic mapping and uranium-series geochronology of fossil coral reefs and geophysical modelling of sea-level records fromWestern Australia.We show that between 127 and 119 kyr ago, eustatic sea level remained relatively stable at about 3–4m above present sea level. However, stratigraphically younger fossil corals with U-series ages of 118.1±1.4kyr are observed at elevations of up to 9.5mabove present mean sea level. Accounting for glacial isostatic adjustment and localized tectonics, we conclude that eustatic sea level rose to about 9mabove present at the end of the last interglacial.We suggest that in the last few thousand years of the interglacial, a critical ice sheet stability threshold was crossed, resulting in the catastrophic collapse of polar ice sheets and substantial sea-level rise.},
author = {Leary, Michael J O and Hearty, Paul J. and Thompson, William G. and Raymo, Maureen E. and Mitrovica, Jerry X. and Webster, Jody M. and O’Leary, Michael J.},
doi = {10.1038/ngeo1890},
file = {:Users/lindzey/Documents/UTIG/Writing/References/UTIG/OLeary2013.pdf:pdf},
isbn = {1752-0908},
issn = {1752-0894},
journal = {Nature Geoscience},
number = {9},
pages = {796--800},
publisher = {Nature Publishing Group},
title = {{Ice sheet collapse following a prolonged period of stable sea level during the last interglacial}},
url = {http://www.nature.com/doifinder/10.1038/ngeo1890},
volume = {6},
year = {2013}
}
