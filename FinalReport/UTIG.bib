@article{Galton-Fenzi2008,
abstract = {Ocean circulation under ice shelves and associated rates of melting$\backslash$nand freezing are strongly influenced by the shape of the sub-ice-shelf$\backslash$ncavity. We have refined an existing method and used additional in$\backslash$nsitu measurements to estimate the cavity shape under the Amery Ice$\backslash$nShelf, East Antarctica. A finite-element hydrodynamic ocean-tide$\backslash$nmodel was used to simulate the major tidal constituents for a range$\backslash$nof different sub-Amery Ice Shelf cavity water-column thicknesses.$\backslash$nThe data are adjusted in the largely unsurveyed southern region of$\backslash$nthe ice-shelf cavity by comparing the complex error between simulated$\backslash$ntides and in situ tides, derived from GPS observations. We show a$\backslash$nsignificant improvement in the simulated tides, with a combined complex$\backslash$nerror of 1.8 cm, in comparison with past studies which show a complex$\backslash$nerror of {\&}{\#}8764;5.3 cm. Our bathymetry incorporates ice-draft data$\backslash$nat the grounding line and seismic surveys, which have provided a$\backslash$nconsiderable amount of new data. This technique has particular application$\backslash$nwhen the water column beneath ice shelves is inaccessible and in$\backslash$nsitu GPS data are available.},
author = {Galton-Fenzi, B K and Maraldi, C and Coleman, R and Hunter, J},
doi = {doi:10.3189/002214308787779898},
