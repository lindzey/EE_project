@article{Hughes2011,
abstract = {Ice sheet thickness is determined mainly by the strength of ice-bed$\backslash$ncoupling that controls holistic transitions from slow sheet flow$\backslash$nto fast streamflow to buttressing shelf flow. Byrd Glacier has the$\backslash$nlargest ice drainage system in Antarctica and is the fastest ice$\backslash$nstream entering Ross Ice Shelf. In 2004 two large subglacial lakes$\backslash$nat the head of Byrd Glacier suddenly drained and increased the terminal$\backslash$nice velocity of Byrd Glacier from 820 m yr(-1) to 900 m yr(-1). This$\backslash$nresulted in partial ice-bed recoupling above the lakes and partial$\backslash$ndecoupling along Byrd Glacier. An attempt to quantify this behavior$\backslash$nis made using flowband and flowline models in which the controlling$\backslash$nvariable for ice height above the bed is the floating fraction phi$\backslash$nof ice along the flowband and flowline. Changes in phi before and$\backslash$nafter drainage are obtained from available data, but more reliable$\backslash$ndata in the map plane are required before Byrd Glacier can be modeled$\backslash$nadequately. A holistic sliding velocity is derived that depends on$\backslash$nphi, with contributions from ice shearing over coupled beds and ice$\backslash$nstretching over uncoupled beds, as is done in state-of-the-art sliding$\backslash$ntheories.},
author = {Hughes, Terence and Sargent, Aitbala and Fastook, James},
doi = {10.1029/2010JF001896},
file = {:Users/lindzey/Documents/UTIG/Writing/References/UTIG/Hughes2011.pdf:pdf},
issn = {21699011},
journal = {Journal of Geophysical Research: Earth Surface},
keywords = {http://dx.doi.org/10.1029/2010JF001896, doi:10.102},
number = {3},
pages = {1--17},
title = {{Ice-bed coupling beneath and beyond ice streams: Byrd Glacier, Antarctica}},
volume = {116},
year = {2011}
}
