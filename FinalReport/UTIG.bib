@article{Behrendt1995,
abstract = {Local maxima of the horizontal gradient of pseudogravity from closely spaced aeromagnetic surveys over the Ross Sea, northwestern Ross Ice Shelf, and the West Antarctic ice sheet, reveal a linear magnetic rift fabric and numerous subcircular, high-amplitude anomalies, Most of these anomalies have sources that probably resulted from late Cenozoic volcanism, Some of these volcanic structures penetrate the Neogene sediments beneath the deglaciated continental shelf and are present at the base of the present grounded ice sheet and beneath the ice shelf, Geophysical data indicate two or three youthful volcanic edifices at widely separated areas beneath the sea and ice cover in the West Antarctic rift system. In contrast, we suggest glacial removal of edifices of volcanic sources of many more anomalies, Magnetic models, controlled by marine seismic reflection and radar ice-sounding data, allow us to infer that glacial removal of the associated late Cenozoic volcanic edifices (probably debris, comprising pillow breccias, and hyaloclastites) has occurred essentially concomitantly with their subglacial eruption, ''Removal'' of unconsolidated volcanic debris erupted beneath the ice is probably a more appropriate term than ''erosion,'' given its fragmented, ice- contact origin, The exposed volcanoes may have been protected from erosion by the surrounding ice sheet because of more competent rock or high elevation above the ice sheet, Glacial removal may be the general case; exposed late Cenozoic volcanic peaks and outcrops, consisting primarily of flows, which erupted during Antarctic glacial conditions since similar to 30 Ma, may be the exceptions, The volume of the exposed volcanoes is small in contrast to the much greater volume (>10(6) km(3)) of late Cenozoic magmatic rock remaining at volcanic centers beneath the continental shelf, Ross Ice Shelf, and West Antarctic ice sheet.},
author = {Behrendt, J C and Blankenship, D D and Damaske, D and Cooper, a K},
file = {:Users/lindzey/Documents/UTIG/Writing/References/UTIG/Behrendt1995.pdf:pdf},
journal = {Geology},
number = {12},
pages = {1111--1114},
title = {{Glacial Removal of Late Cenozoic Subglacially Emplaced Volcanic Edifices by the West Antarctic Ice-Sheet}},
url = {<Go to ISI>://A1995TG96500014},
volume = {23},
year = {1995}
}
@article{Berthier2012,
abstract = {Ice mass loss continues at a high rate among the large glacier$\backslash$ntributaries of the Larsen B Ice Shelf following its disintegration in$\backslash$n2002. We evaluate recent mass loss by mapping elevation changes between$\backslash$n2006 and 2010/11 using differencing of digital elevation models (DEMs).$\backslash$nThe measurement accuracy of these elevation changes is confirmed by a$\backslash$n`null test', subtracting DEMs acquired within a few weeks. The overall$\backslash$n2006-2010/11 mass loss rate (9.0 +/- 2.1 Gt a(-1)) is similar to the$\backslash$n2001/02-2006 rate (8.8 +/- 1.6 Gt a(-1)), derived using DEM differencing$\backslash$nand laser altimetry. This unchanged overall loss masks a varying pattern$\backslash$nof thinning and ice loss for individual glacier basins. On Crane$\backslash$nGlacier, the thinning pulse, initially greatest near the calving front,$\backslash$nis now broadening and migrating upstream. The largest losses are now$\backslash$nobserved for the Hektoria/Green glacier basin, having increased by 33{\%}$\backslash$nsince 2006. Our method has enabled us to resolve large residual$\backslash$nuncertainties in the Larsen B sector and confirm its state of ongoing$\backslash$nrapid mass loss.},
author = {Berthier, Etienne and Scambos, Ted a. and Shuman, Christopher a.},
doi = {10.1029/2012GL051755},
file = {:Users/lindzey/Documents/UTIG/Writing/References/UTIG/Berthier2012.pdf:pdf},
issn = {00948276},
journal = {Geophysical Research Letters},
keywords = {Antarctic Peninsula,IceBridge,Larsen B Ice shelf,SPOT5,sea-level rise,thinning},
number = {13},
pages = {1--6},
title = {{Mass loss of Larsen B tributary glaciers (Antarctic Peninsula) unabated since 2002}},
volume = {39},
year = {2012}
}
