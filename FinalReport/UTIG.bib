@article{Glasser2011,
abstract = {We use optical (ASTER and Landsat) and radar (ERS-1 and ERS-2) satellite$\backslash$nimagery to document changes in the Prince Gustav Ice Shelf, Antarctic$\backslash$nPeninsula, and its tributary glaciers before and after its January$\backslash$n1995 collapse. The satellite image record captures the transition$\backslash$nfrom an ice-shelf glacier system to a tidewater glacial system and$\backslash$nthe subsequent rapid retreat and inferred `fatal' negative mass balances$\backslash$nthat occur as lower glacier elevations lead to higher ablation and$\backslash$ntidewater-style calving collapse. Pre-1995 images show that the central$\backslash$nice shelf was fed primarily by Sjogren Glacier flowing from the Antarctic$\backslash$nPeninsula and by Rohss Glacier flowing from James Ross Island. Numerous$\backslash$nstructural discontinuities (rifts and crevasses) and melt ponds were$\backslash$npresent on the ice shelf before the collapse. After the ice shelf$\backslash$ncollapsed, Rohss Glacier retreated rapidly, becoming a tidewater$\backslash$nglacier in 2002 and receding a total of similar to 15 km between$\backslash$nJanuary 2001 and March 2009, losing >70{\%} of its area. Topographic$\backslash$nprofiles of Rohss Glacier from ASTER-derived digital elevation models$\backslash$nshow a thinning of up to 150 m, and surface speeds increased up to$\backslash$nninefold (0.1-0.9 m d(-1)) over the same period. The rates of speed$\backslash$nincrease and elevation loss, however, are not monotonic; both rates$\backslash$nslowed between late 2002 and 2005, accelerated in 2006 and slowed$\backslash$nagain in 2008-09. We conclude that tributary glaciers react to ice-shelf$\backslash$nremoval by rapid (if discontinuous) recession, and that the response$\backslash$nof tidewater glaciers on the Antarctic Peninsula to ice-shelf removal$\backslash$noccurs over timescales ranging from sub-annual to decadal.},
author = {Glasser, N. F. and Scambos, T. a. and Bohlander, J. and Truffer, M. and Pettit, E. and Davies, B. J.},
doi = {10.3189/002214311796905578},
file = {:Users/lindzey/Documents/UTIG/Writing/References/UTIG/Glasser2011.pdf:pdf},
issn = {00221430},
journal = {Journal of Glaciology},
number = {203},
pages = {397--406},
title = {{From ice-shelf tributary to tidewater glacier: Continued rapid recession, acceleration and thinning of R??hss Glacier following the 1995 collapse of the Prince Gustav Ice Shelf, Antarctic Peninsula}},
volume = {57},
year = {2011}
}
